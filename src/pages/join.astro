---
import JoinUs from "../componentss/ReactComponents/JoinUs";
// import TtlBgImg from "../components/ReactComponents/assets/joinus.jpg"

import Base from "@/layouts/Base.astro";
import PageHeader from "@/partials/PageHeader.astro";
import { getEntry } from "astro:content";

const contact = await getEntry("join", "-index");
// const { Content } = await publications.render();
const { title, description, meta_title, image } = contact.data;


---
<Base
  title={title}
  meta_title={meta_title}
  description={description}
  image={image}
>
  <PageHeader title={title} />



	<!-- <TitleHeader title="Apply To CPS Lab" /> -->

	<JoinUs client:only />
</Base>
